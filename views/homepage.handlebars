        </div>
        <div id="carouselMessage" class="carousel slide" data-ride="carousel" data-interval="3000">
            <ol class="carousel-indicators">
              {{#each review}}
              <li data-target="#carouselMessage" data-slide-to="0" class="active"></li>

                {{/each}}
            </ol>
            <div class="mid-section">
                <div class="carousel-inner">
                  {{#each review}}
                    <div class="carousel-item active">
                        <div class="bg-img">
                        </div>
                        <div>
                            <img class="img-review" src="{{fields.asset.fields.file.url}}" />
                        </div>
                        <div class="text-area">
                            <p>{{fields.description}}</p>
                            <p>
                                <b>
                                    <i>{{fields.name}}</i>
                                </b> - Senior Manager</p>
                        </div>
                    </div>
                    {{/each}}

                </div>
            </div>
          </div>

        <h1 class="four-pillars" id="Toolsused">Tools Used</h1>
        <hr/>
        <div class="row tool-description">
            <div class="section-four">
                <div class="row">
                  {{#each tools}}
                    <div class="col-sm-4 wow zoomIn">
                        <div class="circular-img">
                            <img src="{{fields.icon.asset.fields.file.url}}" />
                        </div>
                        <div class="box">
                            <h3 class="heading-color">{{fields.name}}</h3>
                            <hr/>
                            <p class="text-color" style="text-align:left">{{{fields.description}}}</p>
                            <!-- Button trigger modal -->
                            <button type="button" class="details-button" data-toggle="modal" data-target="#{{sys.id}}">
                                View Details
                            </button>

                            <!-- Modal -->
                            <div class="modal fade" id="{{sys.id}}" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                                <div class="modal-dialog" role="document">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                            </button>
                                        </div>
                                        <div class="modal-body">
                                            <div class="circular-img">
                                                <img src="{{fields.icon.asset.fields.file.url}}" />
                                            </div>

                                            <div class = "text-wrapper">
                                                <h3 class="heading-color">{{fields.name}}</h3>
                                                <p class="text-color">{{fields.description}}</p>
                                            </div>
                                            <div class = "icon-wrapper">
                                              {{#each fields.icon.toolsused}}
                                                <a href="{{fields.description}}" target="_blank"><img class="col-sm-2" src="{{fields.file.url}}"></a>
                                                {{/each}}
                                            </div>
                                        </div>
                                        <div class="modal-footer">

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    {{/each}}

                </div>
            </div>
        </div>

        <h1 class="four-pillars">Get In Touch</h1>
