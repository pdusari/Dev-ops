<div class="bgnd-sec">
        <div class="container-fluid text-center">
            <section class="row content">

                <div class="col-sm-12 text-left">
                    <div class="section-one">
                        <h1>DEVOPS DASHBOARD</h1>
                      <img class="hero-gif" src="./images/DevOps-Portal-Caption.gif">
                        <div class="btn-sec">
                              <a class="btn btn-primary view-industry-btn" href="https://assets.ctfassets.net/qwppyk7r0xj8/5CJn1JEd9e2ewQCMK4KuSi/5da1bfdc6f420aaa02498f04b69e90e7/devOps_Material.pdf" target="_blank">View DevOps Materials</a>
                            <a class="btn btn-primary view-industry-btn" href="/industry">VIEW INDUSTRIES</a>
                        </div>
                    </div>
                </div>
                <div class="modal" id="videoModal" tabindex="-1" role="dialog" aria-labelledby="videoModalLabel" aria-hidden="true">
                    <div class="modal-dialog" role="document">
                      <div class="modal-content">
                        <div class="modal-body">
                            <video controls="controls" preload="auto" id="video1" name="media" style="width: 100%; height: auto; margin:0 auto;">
                                <source src="./images/DevOps Dashboard_lowRes_Mobile.webm" type="video/webm">
                                <source src="./images/DevOps Dashboard_lowRes_Mobile.mp4" type="video/mp4">
                                <source src="./images/DevOps Dashboard_lowRes_Mobile.ogg" type="video/ogg">
                                Your browser doesn't support HTML5 video tag.
                            </video>
                        </div>
                      </div>
                    </div>
                  </div>
            </section>
        </div>
    </div>
    <div class="bgnd-sec2" id="FourPillars">
        <h1 class="four-pillars">Four Pillars</h1>
        <hr/>
        <div class="row section-two">

             {{#each pillars}}
            <div class="col-sm-3 sec-blocks wow zoomIn">
                <div class="circular-img">
                    <img src="{{fields.icon.asset.fields.file.url}}" />
                </div>
                <div class="box">
                    <h3 class="heading-color">{{fields.name}}</h3>
                    <hr/>
                    <p class="text-color" style="text-align:left">{{{fields.description}}}</p>
                </div>
            </div>
                {{/each}}

        </div>

        <h1 class="four-pillars" id="DevelopementCycle">Development Cycle</h1>
        <hr/>
        <div class="section-three">
            <div class="row">
               {{#each devops}}
                <div class="col-sm-3 {{#iffourth @index}} mid-cycle{{/iffourth}}{{#iflast @index}}mid-cycle right{{/iflast}}">
                    <div class="img-box wow zoomIn" style="visibility: visible; animation-name: zoomIn;">
                        <img src="{{fields.icon.asset.fields.file.url}}" {{#iflast @index}} height="90" {{/iflast}}/>
                        <h3 class="img-desc">{{fields.title}}</h3>
                    </div>

                </div>
                {{#ifThird @index}}
              </div>
              <div class="row row-two">
                {{/ifThird}}
                {{/each}}
              </div>
